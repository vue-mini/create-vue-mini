{
  "name": "vue-mini-project",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "format": "prettier --write \"**/*.{js,cjs,ts,json,less,html,md}\"",
    "lint:script": "eslint --ext .js,.cjs,.ts .",
    "lint:style": "stylelint \"src/**/*.less\"",
    "type": "tsc",
    "test": "vitest",
    "dev": "cross-env NODE_ENV=development node build.js",
    "build": "cross-env NODE_ENV=production node build.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "@vue-mini/core": "1.0.0-rc.9",
    "promise-polyfill": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/traverse": "^7.24.5",
    "@babel/types": "^7.24.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^1.5.3",
    "babel-plugin-autocomplete-index": "^0.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-less": "^6.0.0",
    "postcss-pxtorpx-pro": "^2.0.0",
    "postcss-url": "^10.1.3",
    "posthtml": "^0.16.6",
    "rollup": "^4.17.2",
    "serve": "^14.2.3",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3",
    "xo": "^0.58.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint",
        "eslint-plugin-import",
        "webpack"
      ]
    }
  },
  "browserslist": [
    "iOS >= 10",
    "Chrome >= 116"
  ]
}
