{
  "name": "<%- packageName %>",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  <%_ if (packageManager) { _%>
  "packageManager": "<%- packageManager %>",
  <%_ } _%>
  "engines": {
    "node": ">=18.19.1 <19 || >=20.6.1"
  },
  "scripts": {
    <%_ if (needsPrettier) { _%>
    "format": "prettier --write \"**/*.{js<% if (needsEslint) { %>,cjs<% } %><% if (needsTypeScript) { %>,ts<% } %>,json,css,html,md}\"",
    <%_ } _%>
    <%_ if (needsEslint) { _%>
    "lint<% if (needsStylelint) { %>:script<% } %>": "eslint .",
    <%_ } _%>
    <%_ if (needsStylelint) { _%>
    "lint<% if (needsEslint) { %>:style<% } %>": "stylelint \"src/**/*.css\"",
    <%_ } _%>
    <%_ if (needsTypeScript) { _%>
    "type-check": "tsc",
    <%_ } _%>
    <%_ if (needsVitest) { _%>
    "test": "vitest",
    <%_ } _%>
    "dev": "cross-env NODE_ENV=development node build.js",
    "build": "cross-env NODE_ENV=production node build.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@vue-mini/core": "^1.2.8",
    <%_ if (needsPinia) { _%>
    "@vue-mini/pinia": "^1.0.0",
    <%_ } _%>
    trailing-comma
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    <%_ if (needsTypeScript) { _%>
    "@babel/preset-typescript": "^7.28.5",
    <%_ } _%>
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    <%_ if (needsEslint) { _%>
    "@eslint/js": "^9.39.1",
    <%_ } _%>
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    <%_ if (needsTypeScript && needsVitest) { _%>
    "@types/node": "^24.10.1",
    <%_ } _%>
    <%_ if (needsVitest) { _%>
    "@vitest/coverage-v8": "^4.0.14",
    <%_ } _%>
    "babel-plugin-autocomplete-index": "^0.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "chokidar": "^5.0.0",
    "cross-env": "^10.1.0",
    <%_ if (needsEslint) { _%>
    "eslint": "^9.39.1",
    <%_ if (needsPrettier) { _%>
    "eslint-config-prettier": "^10.1.8",
    <%_ } _%>
    <%_ } _%>
    "fs-extra": "^11.3.2",
    <%_ if (needsEslint) { _%>
    "globals": "^16.5.0",
    <%_ } _%>
    "kolorist": "^1.8.0",
    "local-pkg": "^1.1.2",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "postcss-pxtorpx-pro": "^2.0.0",
    <%_ if (needsPrettier) { _%>
    "prettier": "^3.7.2",
    <%_ } _%>
    "rollup": "^4.53.3",
    <%_ if (needsStylelint) { _%>
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    <%_ } _%>
    "terser": "^5.44.1",
    <%_ if (needsTypeScript) { _%>
    "typescript": "^5.9.3",
    <%_ } _%>
    <%_ if (needsTypeScript && needsEslint) { _%>
    "typescript-eslint": "^8.48.0",
    <%_ } _%>
    <%_ if (needsVitest) { _%>
    "vitest": "^4.0.14",
    <%_ } _%>
    trailing-comma
  },
  "browserslist": [
    "iOS >= 10",
    "Chrome >= 63"
  ]
}
